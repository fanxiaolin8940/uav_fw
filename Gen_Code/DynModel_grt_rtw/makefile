# Makefile for the autogenerated code by Matlab

MATLAB_ROOT := /usr/local/MATLAB/R2016a
MATHLABPATH := -I $(MATLAB_ROOT)/simulink/include -I $(MATLAB_ROOT)/extern/include -I ./
.PHONY: DynModel.o DynModel_data.o
CFLAGS += -g

DynModel.o: DynModel.c
	$(CC) $(CFLAGS) $(MATHLABPATH) -c DynModel.c -lm
	cp DynModel.o ../../
	$(CC) $(CFLAGS) $(MATHLABPATH) -c DynModel_data.c
	cp DynModel_data.o ../../

clean: 
	rm *.o
